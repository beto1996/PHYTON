def maximum(a, b, c) :
    if a > b and a > c:
        return a
    elif b > a and b > c:
        return b
    else:
        return c

a = input("ENTER 1st NUMBER\n")
b = input("ENTER 2nd NUMBER\n")
c = input("ENTER 3rd NUMBER\n")
print(maximum(a, b, c)) 




def factorial(n):
    if n == 0:
        return 1
    else:
        return n * factorial(n-1)
n=int(input("Input a number to compute the factiorial : "))
print(factorial(n))



n = int(input("Enter number"))
sum = 0
# loop from 1 to n
for num in range(1, n + 1, 1):
    sum = sum + num
print("Sum of first ", n, "numbers is: ", sum)
average = sum / n
print("Average of ", n, "numbers is: ", average)




# creating an empty list
lst = []

# number of elemetns as input
n = int(input("Enter number of elements : "))

# iterating till the range
for i in range(0, n):
    ele = int(input())

    lst.append(ele)  # adding the element

print(lst)




# Python3 program to count the number of times
# an object appears in a list using count() method 
  
list1 = [1, 1, 1, 2, 3, 2, 1] 
  
# Counts the number of times 1 appears in list1
print(list1.count(1)) 
  
list2 = ['a', 'a', 'a', 'b', 'b', 'a', 'c', 'b'] 
  
# Counts the number of times 'b' appears in list2
print(list2.count('b')) 
  
list3 = ['Cat', 'Bat', 'Sat', 'Cat', 'cat', 'Mat']
  
# Counts the number of times 'Cat' appears in list3
print(list3.count('Cat'))





# Creating a List
List = []
print("Blank List: ")
print(List)
  
# Creating a List of numbers
List = [10, 20, 14]
print("\nList of numbers: ")
print(List)
  
# Creating a List of strings and accessing
# using index
List = ["Geeks", "For", "Geeks"]
print("\nList Items: ")
print(List[0]) 
print(List[2])
  
# Creating a Multi-Dimensional List
# (By Nesting a list inside a List)
List = [['Geeks', 'For'] , ['Geeks']]
print("\nMulti-Dimensional List: ")
print(List)




import random

x = random.random()
# Random float number
for i in range(3):
    print(random.random())


def cube(num):
    return num * num * num

number = float(input(" Please Enter any numeric Value : "))

cub = cube(number)

print("The Cube of a Given Number {0}  = {1}".format(number, cub))

